// -*- mode: Java; -*-
package sanka.http;

/**
 * HttpRequest represents an incoming request, ready to be processed.
 */
class HttpRequest {
    String method;
    String uri;
    String httpVersion;
    map[String]String headers;
    map[String]String cookies;
    byte[] body;

    HttpRequest() {
        this.headers = new map[String]String;
    }
}
