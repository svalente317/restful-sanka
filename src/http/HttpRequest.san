// -*- mode: Java; -*-
package sanka.http;

/**
 * HttpRequest represents an incoming request, ready to be processed.
 */
class HttpRequest {
    String method;
    String uri;
    String httpVersion;
    String[String] headers;
    String[String] cookies;
    byte[] body;

    HttpRequest() {
        this.headers = new String[class String];
    }
}
